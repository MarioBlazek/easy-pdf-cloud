{
    "name": "marioblazek/easy-pdf-cloud",
    "description": "PHP easyPDF Cloud API",
    "type": "library",
    "keywords": [
        "bcl",
        "easyPDF"
    ],
    "homepage": "https://github.com/MarioBlazek/easy-pdf-cloud",
    "license": "MIT",
    "authors": [
        {
            "name": "BCL Technologies",
            "homepage": "https://www.easypdfcloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bcl\\EasyPdfCloud\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bcl\\EasyPdfCloud\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "~2.0",
        "symfony/var-dumper": "~3.0|~4.0"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --color=always",
        "fix": "@php vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
